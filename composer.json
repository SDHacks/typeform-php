{
    "name": "sdhacks/typeform-php",
    "description": "A library for the Typeform API",
    "keywords": [
        "typeform-php"
    ],
    "homepage": "https://github.com/sdhacks/typeform-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Le",
            "email": "joseph@sdhacks.io",
            "homepage": "josephle.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Typeform\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Typeform\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
